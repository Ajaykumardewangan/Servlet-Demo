package com.java.service;

public class EmpService {  
private int id;  
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}

private String name;  
public String getEmail() {
	return email;
}

private String email;  
//getters and setters  
} 